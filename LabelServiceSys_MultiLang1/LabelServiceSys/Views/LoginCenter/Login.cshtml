@using LabelServiceSys.Models;
<script type="text/javascript">
@Html.Raw(LangHelper.WhichLang());
@Html.Raw(Html.LangOutJsVar("LoginCenter_JS_Login_ErrorMessage1"));
@Html.Raw(Html.LangOutJsVar("LoginCenter_JS_Login_ErrorMessage2"));
</script>
@model Model.M_LoginReturn
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@Html.GetLangbyKey("LoginCenter_Html_Index_Login_Title")</title>
    <script src="@Url.Content("~/Scripts/jquery-1.6.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JS/LoginCenter_Login.js")" type="text/javascript"></script>
</head>
<body style="padding: 0px; background-color: #FFFF80; font-size: 10px">
    <table style="padding: 0px; font-size:11px">
        <tr>
            <td>
                <span id="lblErrorTip"></span>
            </td>
        </tr>
        <tr>
            <td>
                @Html.GetLangbyKey("LoginCenter_Html_Index_Login_UserName"):
                <input  type="hidden" id="txtIsLogin" name="txtIsLogin" value="@Model.IsLogin_i"/>
                <input  type="hidden" id="txtUserNumber" name="txtUserNumber" value="@Model.UserNumber"/>
                <input  type="hidden" id="txtIsDVIR" name="txtIsDVIR" value="@Model.IsDVIR"/>
                 <input  type="hidden" id="hd_getWorkstationId" name="hd_getWorkstationId" value="@Model.getWorkstationId"/>
                 <input  type="hidden" id="hd_WorkstationId" name="hd_WorkstationId" value="@Model.WorkstationId"/>
                 <input  type="hidden" id="hd_WorkprocessL1_Code" name="hd_WorkprocessL1_Code" value="@Model.WorkProcess_L1_Code"/>
                 <input  type="hidden" id="hd_WorkprocessL1_Text" name="hd_WorkprocessL1_Text" value="@Model.WorkProcess_L1_Text"/>
                 <input  type="hidden" id="hd_WorkprocessL2_Code" name="hd_WorkprocessL2_Code" value="@Model.WorkProcess_L2_Code"/>
                 <input  type="hidden" id="hd_WorkprocessL2_Text" name="hd_WorkprocessL2_Text" value="@Model.WorkProcess_L2_Text"/>
            </td>
        </tr>
        <tr>
            <td>
                &nbsp;&nbsp;&nbsp;&nbsp;<span id="spanUserName">@Model.UserNumber</span>
            </td>
        </tr>
        <tr>
            <td>
                @Html.GetLangbyKey("LoginCenter_Html_Index_Login_HasDVIR"):@Model.IsDVIRDesc
            </td>
        </tr>
        <tr>
            <td>
                @Html.GetLangbyKey("LoginCenter_Html_Index_Login_JobContent1"):
            </td>
        </tr>
        <tr>
            <td>
                &nbsp;&nbsp;<select id="ddlWorkProcess_L1_Code" name="ddlWorkProcess_L1_Code" style="width: 145px">
                </select>
            </td>
        </tr>
        <tr>
            <td>
                @Html.GetLangbyKey("LoginCenter_Html_Index_Login_JobContent2"):
            </td>
        </tr>
        <tr>
            <td>
                &nbsp;&nbsp;<select id="ddlWorkProcess_L2_Code" name="ddlWorkProcess_L2_Code" style="width: 145px">
                </select>
            </td>
        </tr>
        <tr>
            <td align="center">
                <input type="button" id="btnLogin" name="btnLogin" value="@Html.GetLangbyKey("LoginCenter_Html_Index_Login_btnLogin")" style="width: 100px" />
            </td>
        </tr>
    </table>
</body>
</html>
